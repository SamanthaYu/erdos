<% provide(:title, "Log in") %>
<h1>Log in</h1>

    <p id="notice"><%= notice %></p>

    <%= form_for(:session, url: login_path) do |f| %>

      <%= f.text_field :username %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>
