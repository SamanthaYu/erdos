<%= form_for chatroom, remote: true do |f| %>
  <h3>Create a New Chatroom</h3>
  <% f.label :roomname%>
  <%= f.text_field :roomname, :placeholder => 'Enter Chatroom Name'%>
  <%= f.hidden_field :user_id, value: current_user.id %>
  <center><%= f.submit "Create Chatroom", class: "submitButton"%></center>
<% end %>
